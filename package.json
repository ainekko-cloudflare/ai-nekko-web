{
  "name": "@ai-nekko/root",
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/front"
    ],
    "nohoist": [
      "**/@strapi/strapi",
      "**/@strapi/**"
    ]
  },
  "scripts": {
    "build": "turbo run build --filter=apps/front",
    "dev": "turbo run dev --filter=apps/front",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "devDependencies": {
    "turbo": "1.10.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "ajv": "^8.12.0"
  },
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@hello-litebox/commit-prompt": "^1.1.6",
    "@hello-litebox/commitlint": "^1.1.4",
    "ajv": "^8.17.1",
    "commitizen": "^4.3.0",
    "husky": "^8.0.0",
    "sharp": "^0.33.5"
  },
  "packageManager": "yarn@1.22.19",
  "config": {
    "commitizen": {
      "path": "@hello-litebox/commit-prompt"
    }
  }
}
